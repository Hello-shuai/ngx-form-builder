{
    "debug": true,
    "modelName": "testModel",
    "span_label": 4,
    "span": 8,
    "grid": {
        "md": {
            "span": 6
        }
    },
    "properties": {
        "email": {
            "type": "string",
            "title": "邮箱",
            "format": "email",
            "placeholder": "请输入邮箱，最多20个字符",
            "maxLength": 20,
            "readOnly": true,
            "grid": {
                "md": {
                    "span": 4
                },
                "lg": {
                    "span": 6
                }
            },
            "default": "giscafer@outlook.com"
        },
        "name": {
            "type": "string",
            "title": "姓名",
            "placeholder": "请输入姓名",
            "description": "必须大写开头且3个字以上",
            "maxLength": 30,
            "minLength": 3,
            "debug": true
        },
        "age": {
            "type": "number",
            "title": "年龄",
            "minimum": 1,
            "maximum": 100
        },
        "sex": {
            "type": "string",
            "description": "性别",
            "widget": "radio",
            "grid": {
                "mode": "inline",
                "md":{
                    "span": 6
                }
            },
            "oneOf": [
                {
                    "description": "男",
                    "enum": [
                        "male"
                    ]
                },
                {
                    "description": "女",
                    "enum": [
                        "female"
                    ]
                }
            ]
        },
        "remark": {
            "type": "string",
            "title": "描述",
            "grid": {
                "md": {
                    "span": 12
                },
                "lg": {
                    "span": 12
                }
            }
        }
    },
    "buttons": [
        {
            "label": "Alert",
            "id": "alert",
            "submit": true,
            "offset": 4,
            "grid": {
                "md": {
                    "offset": 4,
                    "span": 6
                }
            }
        },
        {
            "label": "Reset",
            "id": "reset"
        }
    ]
}